<launch>
    <arg name="video_device"/>
    <arg name="run_nav" default="true"/>

    <include file="$(find demo7)/launch/ar_track_midcam.launch"/>
    <include file="$(find demo7)/launch/ar_track_topcam.launch"/>
    <include file="$(find demo7)/launch/odom_nav_demo.launch"/>
    <include file="$(find turtlebot_bringup)/launch/3dsensor.launch"/>
    <include file="$(find turtlebot_bringup)/launch/minimal.launch"/>      

    <node name="usb_cam" pkg="usb_cam" type="usb_cam_node" output="screen" >
        <param name="video_device" value="$(arg video_device)" />
        <param name="image_width" value="640" />
        <param name="image_height" value="480" />
        <param name="pixel_format" value="yuyv" />
        <param name="camera_frame_id" value="usb_cam_frame" />
        <param name="io_method" value="mmap"/>
    </node>
    <node name="image_view" pkg="image_view" type="image_view" respawn="false" output="screen">
        <remap from="image" to="/usb_cam/image_raw"/>
        <param name="autosize" value="true" />
    </node>

</launch>
